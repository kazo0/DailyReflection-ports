<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DailyReflection.Avalonia.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:DailyReflection.Avalonia.Views"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="700"
        x:Class="DailyReflection.Avalonia.Views.MainWindow"
        x:DataType="vm:MainViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="Daily Reflection"
        Width="400"
        Height="700"
        MinWidth="350"
        MinHeight="500">
        <views:MainView DataContext="{Binding}" />
</Window>
